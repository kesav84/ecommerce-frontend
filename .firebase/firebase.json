{
    "hosting": {
      "public": "public",
      "headers": [
        {
          "source": "**/*.@(js|css|webp|jpg|jpeg|png|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public,max-age=31536000,immutable"
            }
          ]
        }
      ]
    }
  }
  